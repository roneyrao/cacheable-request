{
  "name": "cacheable-request-adaptable",
  "version": "3.0.2",
  "description": "Wrap native HTTP requests with RFC compliant cache support",
  "main": "dist",
  "files": [
    "/dist"
  ],
  "scripts": {
    "test": "xo && nyc ava --verbose --reporter=text --reporter=lcov",
    "prebuild": "npm test",
    "build": "babel src -d dist",
    "prepublish": "npm build"
  },
  "xo": {
    "extends": "xo-lukechilds"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/roneyrao/cacheable-request-adaptable.git"
  },
  "keywords": [
    "HTTP",
    "HTTPS",
    "cache",
    "caching",
    "layer",
    "cacheable",
    "RFC 7234",
    "RFC",
    "7234",
    "compliant"
  ],
  "author": "Roney Rao",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/roneyrao/cacheable-request-adaptable/issues"
  },
  "homepage": "https://github.com/roneyrao/cacheable-request-adaptable",
  "dependencies": {
    "clone-response": "1.0.2",
    "get-stream": "3.0.0",
    "http-cache-semantics": "3.8.1",
    "keyv": "3.0.0",
    "lowercase-keys": "1.0.0",
    "normalize-url": "2.0.1",
    "responselike": "1.0.2"
  },
  "devDependencies": {
    "@keyv/sqlite": "^1.2.6",
    "ava": "^0.25.0",
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-register": "^6.26.0",
    "create-test-server": "^2.0.0",
    "delay": "^2.0.0",
    "eslint-config-xo-lukechilds": "^1.0.0",
    "nyc": "^11.0.2",
    "pify": "^3.0.0",
    "sqlite3": "^3.1.9",
    "xo": "^0.19.0"
  }
}
